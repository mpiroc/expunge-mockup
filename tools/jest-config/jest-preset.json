{
    "preset": "ts-jest",
    "globals": {
        "ts-jest": {
            "packageJson": "package.json"
        }
    },
    "roots": [
        "<rootDir>/test"
    ],
    "testEnvironment": "node",
    "testMatch": [
        "**/*.test.ts"
    ],
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
        "json",
        "html"
    ]
}